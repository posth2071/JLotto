<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/expand_One_Rank"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="test"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_weight="1"
            ></TextView>
        <TextView
            android:id="@+id/expand_One_TotalMoney"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="test"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_weight="1"
            ></TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/expand_One_People"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="test"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_weight="1"
            ></TextView>
        <TextView
            android:id="@+id/expand_One_PersonalMoney"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="test"
            android:textSize="25dp"
            android:gravity="center"
            android:layout_weight="1"
            ></TextView>
    </LinearLayout>

    <TextView
        android:id="@+id/expand_One_Norm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="25dp"
        android:gravity="center"
        ></TextView>

</LinearLayout>
